{
  "name": "affiliate-dashboard",
  "version": "1.0.0",
  "description": "A comprehensive affiliate marketing dashboard for managing programs, tracking commissions, and generating marketing materials",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "keywords": ["affiliate-marketing", "commission-tracking", "marketing-tools", "dashboard"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "stripe": "^13.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "express-validator": "^7.0.1",
    "moment": "^2.29.4",
    "sharp": "^0.32.1",
    "qrcode": "^1.5.3",
    "puppeteer": "^20.8.2",
    "canvas": "^2.11.2",
    "uuid": "^9.0.0",
    "crypto": "^1.0.1",
    "node-cron": "^3.0.2",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
