{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/build/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/client/build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/zanwik-icon.svg",
      "dest": "/public/zanwik-icon.svg"
    },
    {
      "src": "/asset-manifest.json",
      "dest": "/client/build/asset-manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/client/build/sw.js"
    },
    {
      "src": "/dashboard",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/blog",
      "dest": "/api/index.js"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/bpm-login",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}